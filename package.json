{
  "name": "youtube-react",
  "version": "1.0.0",
  "description": "Full Stack Youtube (PERN) Postgres, Express, React & Node",
  "engines": {
    "node": "16.16.0"
  },
  "scripts": {
    "start": "node server",
    "dev:server": "nodemon server",
    "dev:client": "cd client && npm start",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "build": "babel --delete-dir-on-start --out-dir dist --copy-files --ignore \"**/__tests__/**, **/__mocks__/**\" --no-copy-ignored server/src",
    "save": "prisma migrate --experimental save",
    "up": "prisma migrate --experimental up",
    "generate": "prisma generate",
    "studio": "prisma studio"
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ],
  "author": "Chris Risner",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "google-auth-library": "^8.1.1",
    "jsonwebtoken": "^8.5.1",
    "loglevel": "^1.8.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.9",
    "@babel/core": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/register": "^7.18.9",
    "@prisma/cli": "2.12.0",
    "concurrently": "^7.3.0",
    "nodemon": "^2.0.19",
    "prisma": "^4.1.1"
  }
}
